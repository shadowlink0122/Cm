// std::web - ブラウザ向け簡易UI

module std::web;

use "js" {
    void cm_web_set_html(string html);
    void cm_web_append_html(string html);
    void cm_web_set_css(string css);
    string cm_web_get_html();
}

export void set_html(string html) {
    cm_web_set_html(html);
}

export void append_html(string html) {
    cm_web_append_html(html);
}

export void set_css(string css) {
    cm_web_set_css(css);
}

export string get_html() {
    return cm_web_get_html();
}
