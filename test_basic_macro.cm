// 基本的なマクロテスト

// シンプルなマクロ定義
#macro void LOG_INT(int x) {
    println(x);
}

// テスト関数
#test void test_addition() {
    int a = 1;
    int b = 2;
    int c = a + b;
}

// ベンチマーク関数
#bench void benchmark_loop() {
    int sum = 0;
    for (int i = 0; i < 1000; i++) {
        sum = sum + i;
    }
}

// メイン関数
int main() {
    LOG_INT(42);
    return 0;
}