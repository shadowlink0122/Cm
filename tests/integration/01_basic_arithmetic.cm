// Basic arithmetic operations test
int main() {
    int a = 10;
    int b = 20;
    int sum = a + b;
    int diff = b - a;
    int prod = a * 5;
    int quot = b / 4;

    // Expected: 30
    println(sum);
    // Expected: 10
    println(diff);
    // Expected: 50
    println(prod);
    // Expected: 5
    println(quot);

    return 0;
}