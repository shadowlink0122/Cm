// Minimal bubble sort test
import std::io;

void bubble_sort(int* arr, int n) {
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}

int main() {
    int n = 5;
    int[5] arr = [5, 4, 3, 2, 1];
    
    bubble_sort(arr, n);
    
    println("Sorted: {arr[0]}, {arr[1]}, {arr[2]}, {arr[3]}, {arr[4]}");
    return 0;
}
