// test_minimal_generic.cm - 最小ジェネリック構造体テスト
import std::io::println;

// 最小のジェネリック構造体
struct Box<T> {
    T value;
}

// ジェネリック関数
<T> T get_value(Box<T>* box) {
    return box->value;
}

int main() {
    println("Test start");
    
    // int版のBox
    Box<int> box_int;
    box_int.value = 42;
    
    int v = get_value(&box_int);
    println("box_int.value = {v}");
    
    println("Test done");
    return 0;
}
