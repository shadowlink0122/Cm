// ベンチマーク2: フィボナッチ数列（再帰版）
// 35番目のフィボナッチ数を計算（再帰の深さをテスト）

import std::io;

int fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int n = 30;
    int result = fibonacci(n);

    println("Fibonacci({n}) = {result}");
    return 0;
}