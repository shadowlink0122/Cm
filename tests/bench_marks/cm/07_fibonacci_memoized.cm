// ベンチマーク7: フィボナッチ数列（動的計画法版）
// 100番目のフィボナッチ数を効率的に計算

import std::io;

int main() {
    int n = 45;  // int範囲内に収まる値

    // 動的計画法でフィボナッチを計算
    if (n <= 0) {
        println("Fibonacci({n}) = 0");
        return 0;
    }

    if (n == 1) {
        println("Fibonacci({n}) = 1");
        return 0;
    }

    int prev2 = 0;
    int prev1 = 1;
    int result = 0;

    for (int i = 2; i <= n; i++) {
        result = prev1 + prev2;
        prev2 = prev1;
        prev1 = result;
    }

    println("Fibonacci({n}) with DP = {result}");

    return 0;
}