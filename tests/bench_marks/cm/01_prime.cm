// ベンチマーク1: 素数計算
// 100,000までの素数を全てカウント

import std::io;

bool is_prime(int n) {
    if (n <= 1) {
        return false;
    }
    if (n <= 3) {
        return true;
    }
    if (n % 2 == 0 || n % 3 == 0) {
        return false;
    }

    int i = 5;
    while (i * i <= n) {
        if (n % i == 0 || n % (i + 2) == 0) {
            return false;
        }
        i = i + 6;
    }
    return true;
}

int main() {
    int limit = 100000;
    int count = 0;

    for (int i = 2; i <= limit; i++) {
        if (is_prime(i)) {
            count++;
        }
    }

    println("Prime numbers up to {limit}: {count}");
    return 0;
}