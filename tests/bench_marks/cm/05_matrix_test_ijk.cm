// 行列乗算テスト - IJK順のみ
import std::io;

int main() {
    int n = 500;
    println("Testing IJK order with 2D array [500][500]...");

    // 2次元配列として宣言（スタック上）
    int[500][500] a;
    int[500][500] b;
    int[500][500] c;

    // 行列初期化
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            a[i][j] = (i + j) % 100;
            b[i][j] = (i - j + 100) % 100;
            c[i][j] = 0;
        }
    }

    // 行列乗算（標準的なIJK順）
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            int sum = 0;
            for (int k = 0; k < n; k++) {
                sum = sum + a[i][k] * b[k][j];
            }
            c[i][j] = sum;
        }
    }

    // 結果の一部を表示
    println("Result c[0][0] = {c[0][0]}");
    println("Result c[499][499] = {c[499][499]}");
    println("Completed");
    return 0;
}