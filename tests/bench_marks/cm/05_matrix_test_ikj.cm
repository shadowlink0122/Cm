// 行列乗算テスト - IKJ順のみ
import std::io;

int main() {
    int n = 500;
    println("Testing IKJ order with 2D array [500][500]...");

    // 2次元配列として宣言（スタック上）
    int[500][500] a;
    int[500][500] b;
    int[500][500] c;

    // 行列初期化
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            a[i][j] = (i + j) % 100;
            b[i][j] = (i - j + 100) % 100;
            c[i][j] = 0;
        }
    }

    // 行列乗算（IKJ順）
    for (int i = 0; i < n; i++) {
        for (int k = 0; k < n; k++) {
            int a_ik = a[i][k];
            for (int j = 0; j < n; j++) {
                c[i][j] = c[i][j] + a_ik * b[k][j];
            }
        }
    }

    // 結果の一部を表示
    println("Result c[0][0] = {c[0][0]}");
    println("Result c[499][499] = {c[499][499]}");
    println("Completed");
    return 0;
}