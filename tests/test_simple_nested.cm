// test_simple_nested.cm - シンプルなネストフィールドテスト
import std::io::println;

struct Item { int value; }
struct Node { Item data; }

int main() {
    println("Test");
    
    Node node;
    node.data.value = 42;  // ネストフィールドへの書き込み
    
    int v = node.data.value;  // ネストフィールドからの読み込み
    println("v = {v}");
    
    return 0;
}
