// where句を使ったジェネリック構造体のテスト
// struct Container<T> with Eq where T: Eq

struct Container<T> with Eq where T: Eq {
    T value;
}

int main() {
    Container<int> c1;
    c1.value = 42;
    
    Container<int> c2;
    c2.value = 42;
    
    Container<int> c3;
    c3.value = 100;
    
    // c1 == c2 は true
    if (c1 == c2) {
        println("c1 == c2: true");
    } else {
        println("c1 == c2: false");
    }
    
    // c1 == c3 は false
    if (c1 == c3) {
        println("c1 == c3: true");
    } else {
        println("c1 == c3: false");
    }
    
    return 0;
}
