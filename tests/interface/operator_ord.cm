// operator < のテスト
// struct with Ord による自動実装

struct Point with Eq, Ord {
    int x;
    int y;
}

int main() {
    Point p1;
    p1.x = 1;
    p1.y = 2;
    
    Point p2;
    p2.x = 3;
    p2.y = 4;
    
    Point p3;
    p3.x = 1;
    p3.y = 2;
    
    // p1 < p2 は true (辞書順比較)
    if (p1 < p2) {
        println("p1 < p2: true");
    } else {
        println("p1 < p2: false");
    }
    
    // p2 < p1 は false
    if (p2 < p1) {
        println("p2 < p1: true");
    } else {
        println("p2 < p1: false");
    }
    
    // p1 < p3 は false (等しい)
    if (p1 < p3) {
        println("p1 < p3: true");
    } else {
        println("p1 < p3: false");
    }
    
    // p1 > p2 は false（>は<から自動導出）
    if (p1 > p2) {
        println("p1 > p2: true");
    } else {
        println("p1 > p2: false");
    }
    
    // p1 <= p3 は true（<=は<から自動導出）
    if (p1 <= p3) {
        println("p1 <= p3: true");
    } else {
        println("p1 <= p3: false");
    }
    
    // p1 >= p3 は true（>=は<から自動導出）
    if (p1 >= p3) {
        println("p1 >= p3: true");
    } else {
        println("p1 >= p3: false");
    }
    
    return 0;
}
