// Phase 2: if-else文のテスト
int main() {
    int x = 10;

    // else節が実行されない
    if (x > 5) {
        println(1);
    } else {
        println(2);
    }

    // else節が実行される
    if (x < 5) {
        println(3);
    } else {
        println(4);
    }

    // 変数を変更してテスト
    x = 3;
    if (x < 5) {
        println(5);
    } else {
        println(6);
    }

    return 0;
}