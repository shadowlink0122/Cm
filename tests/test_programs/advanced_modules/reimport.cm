// reimport.cm - モジュール再インポート防止のテスト
// 同じモジュールを複数の形式でインポートしても正しく動作することを確認

import std::io::println;

// vectorモジュールを複数の形式でインポート
import std::collections::vector::*;       // ワイルドカード
import std::collections::vector::Vector;  // 選択的

int main() {
    println("Testing reimport prevention:");

    // Vector<int>を使用（複数インポートでも問題なく動作）
    Vector<int> v1();
    v1.push(10);
    v1.push(20);

    int len = v1.len();
    int val0 = *v1.get(0);
    int val1 = *v1.get(1);
    println("Vector<int>: len={len}, values=[{val0}, {val1}]");

    // 2つ目のVectorを作成
    Vector<int> v2();
    v2.push(100);

    int len2 = v2.len();
    int val2 = *v2.get(0);
    println("Vector<int> (second): len={len2}, value={val2}");

    // デストラクタが自動的に呼ばれる

    println("Success: Multiple imports work correctly");
    println("=== PASS ===");
    return 0;
}
