// test_complete_types.cm - 完全なモジュールテスト

import complete_types;

int main() {
    // 構造体の使用
    float x = 3.0;
    float y = 4.0;
    Vector2D v = createVector(x, y);
    println("Vector created: ({v.x:.1}, {v.y:.1})");

    float len = vectorLength(v);
    println("Vector squared length: {len:.2}");

    // Enumの使用
    Direction d = Direction::North;
    printDirection(d);

    Direction opposite = oppositeDirection(d);
    printDirection(opposite);

    // 複数の方向をテスト
    Direction east = Direction::East;
    printDirection(oppositeDirection(east));

    return 0;
}