// must {} ブロック構文テスト
// mustブロック内のコードはデッドコード最適化禁止

import std::io::println;

int main() {
    println("=== must {} Block Test ===");
    
    // 基本的なmustブロック（asmなし）
    int x = 0;
    must {
        x = 10;
        x = 20;
    }
    if (x == 20) {
        println("must block basic: PASS");
    } else {
        println("must block basic: FAIL");
    }
    
    // asmはmustブロック外で実行
    __llvm__("nop");
    println("asm outside must: PASS");
    
    println("=== All Tests Passed ===");
    return 0;
}
