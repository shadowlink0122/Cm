// インラインアセンブリ基本テスト
import std::asm::{nop, barrier, pause, is_x86, is_arm64};
import std::io::println;

int main() {
    println("=== Inline Assembly Test ===");
    
    // 基本命令
    nop();
    barrier();
    pause();
    
    // アーキテクチャ検出
    if (is_x86()) {
        println("x86 detected");
    }
    if (is_arm64()) {
        println("ARM64 detected");
    }
    
    println("=== Complete ===");
    return 0;
}
