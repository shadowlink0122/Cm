// whileループのテスト
int main() {
    // 基本的なwhileループ
    int i = 0;
    while (i < 5) {
        println(i);
        i = i + 1;
    }

    // 条件が最初からfalseの場合
    int j = 10;
    while (j < 5) {
        println(999);  // 実行されない
    }

    // ネストされたループ
    int x = 0;
    while (x < 2) {
        int y = 0;
        while (y < 3) {
            int sum = x * 10 + y;
            println(sum);
            y = y + 1;
        }
        x = x + 1;
    }

    return 0;
}