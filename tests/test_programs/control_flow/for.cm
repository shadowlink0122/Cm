// Phase 2: forループのテスト（未実装）
import std::io::println;
int main() {
    // 基本的なforループ
    for (int i = 0; i < 5; i = i + 1) {
        println(i);
    }

    // 逆順ループ
    for (int j = 5; j > 0; j = j - 1) {
        println(j);
    }

    // ステップが2
    for (int k = 0; k < 10; k = k + 2) {
        println(k);
    }

    return 0;
}
