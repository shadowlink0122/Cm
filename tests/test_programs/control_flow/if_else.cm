// if-else文のテスト
int main() {
    int x = 10;

    // 単純なif文
    if (x > 5) {
        println(1);
    }

    // if-else文
    if (x < 5) {
        println(0);
    } else {
        println(2);
    }

    // else if連鎖
    if (x < 10) {
        println(0);
    } else if (x == 10) {
        println(3);
    } else {
        println(0);
    }

    // ネストされたif
    if (x > 0) {
        if (x <= 10) {
            println(4);
        }
    }

    return 0;
}