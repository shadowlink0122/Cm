import std::io::println;
// for-in（範囲for）構文のテスト

int main() {
    int[5] arr;
    arr[0] = 10;
    arr[1] = 20;
    arr[2] = 30;
    arr[3] = 40;
    arr[4] = 50;
    
    // for-in構文（型指定あり）
    int sum = 0;
    for (int val in arr) {
        println("val = {val}");
        sum = sum + val;
    }
    println("sum = {sum}");
    
    return 0;
}
