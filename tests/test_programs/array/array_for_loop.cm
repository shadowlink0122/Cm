import std::io::println;
// forループで配列をイテレート

int main() {
    int[5] arr;
    arr[0] = 10;
    arr[1] = 20;
    arr[2] = 30;
    arr[3] = 40;
    arr[4] = 50;
    
    // インデックスベースのforループ
    int sum = 0;
    for (int i = 0; i < 5; i = i + 1) {
        int val = arr[i];
        println("arr[{i}] = {val}");
        sum = sum + arr[i];
    }
    println("sum = {sum}");
    
    // size()を使ったforループ
    int sum2 = 0;
    uint size = arr.size();
    for (int j = 0; j < size; j = j + 1) {
        sum2 = sum2 + arr[j];
    }
    println("sum2 = {sum2}");
    
    return 0;
}
