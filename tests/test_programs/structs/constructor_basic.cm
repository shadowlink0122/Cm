// コンストラクタのテスト

struct Point {
    int x;
    int y;
}

impl Point {
    self() {
        self.x = 0;
        self.y = 0;
    }
    
    overload self(int a, int b) {
        self.x = a;
        self.y = b;
    }
}

int main() {
    Point p1;
    p1.x = 1;
    p1.y = 2;
    println("p1: ({}, {})", p1.x, p1.y);
    
    return (p1.x == 1 && p1.y == 2) ? 0 : 1;
}
