// private修飾子のテスト
// privateメンバは外部から直接アクセス不可

import std::io::println;

struct Person {
    string name;        // 外部アクセス可能
    private int age;    // 外部から直接アクセス不可（将来的にエラー）
}

int main() {
    Person p;
    p.name = "Alice";
    
    // publicメンバは直接アクセス可能
    println("Name: {}", p.name);

    return 0;
}
