// default修飾子の暗黙的アクセステスト
// 構造体のdefaultメンバへの暗黙的なアクセス

struct Wrapper {
    default int value;
    string tag;
}

int main() {
    Wrapper w;
    w.value = 42;
    w.tag = "test";

    // defaultメンバへの暗黙的な代入: w = 100 → w.value = 100
    w = 100;

    // defaultメンバからの暗黙的な取得: int x = w → int x = w.value
    int x = w;

    println("w.value = {w.value}");
    println("x = {x}");

    // 期待値チェック
    return (w.value == 100 && x == 100) ? 0 : 1;
}
