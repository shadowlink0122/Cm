// コンストラクタ呼び出し構文のテスト
// Type name(args) 形式

struct Point {
    int x;
    int y;
}

impl Point {
    self() {
        self.x = 0;
        self.y = 0;
    }
    
    overload self(int a, int b) {
        self.x = a;
        self.y = b;
    }
}

int main() {
    // 引数付きコンストラクタ呼び出し
    Point p(10, 20);
    
    println("p: ({p.x}, {p.y})");
    
    return (p.x == 10 && p.y == 20) ? 0 : 1;
}
