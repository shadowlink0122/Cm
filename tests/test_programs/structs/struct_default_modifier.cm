// default修飾子のテスト
// defaultメンバは構造体に1つだけ設定可能
// メンバ名省略時にデフォルトメンバにアクセス

struct Wrapper {
    default int value;  // デフォルトメンバ
    string tag;
}

struct Counter {
    string name;
    default int count;  // デフォルトメンバ
}

int main() {
    Wrapper w;
    w.value = 42;
    w.tag = "test";

    Counter c;
    c.name = "hits";
    c.count = 100;

    // default修飾子のテスト：直接アクセス
    w.value = 50;
    c.count = c.count + 1;

    // 通常のアクセスが正しく動作することを確認
    return (w.value == 50 && c.count == 101) ? 0 : 1;
}
