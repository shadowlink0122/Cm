// 多次元配列のテスト
import std::io::println;

int main() {
    // 2次元配列（固定サイズ）
    int[3][3] matrix = [[1, 2, 3], [4, 5, 6], [7, 8, 9]];
    
    // 直接アクセス
    println("matrix[0][0]: {matrix[0][0]}");
    println("matrix[1][1]: {matrix[1][1]}");
    println("matrix[2][2]: {matrix[2][2]}");
    
    // 行を取得（固定サイズ配列からスライスへ）
    int[] row0 = matrix[0];
    int[] row1 = matrix[1];
    int[] row2 = matrix[2];
    
    println("row0: {row0[0]}, {row0[1]}, {row0[2]}");
    println("row1: {row1[0]}, {row1[1]}, {row1[2]}");
    println("row2: {row2[0]}, {row2[1]}, {row2[2]}");
    
    // 合計を計算
    int sum = 0;
    for (int i = 0; i < 3; i = i + 1) {
        for (int j = 0; j < 3; j = j + 1) {
            sum = sum + matrix[i][j];
        }
    }
    println("sum: {sum}");
    
    return 0;
}
