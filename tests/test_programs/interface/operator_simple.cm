import std::io::println;
// シンプルなoperator実装テスト
struct Point {
    int x;
    int y;
}

impl Point for Eq {
    operator bool ==(Point other) {
        return self.x == other.x && self.y == other.y;
    }
}

int main() {
    Point p1;
    p1.x = 10;
    p1.y = 20;

    Point p2;
    p2.x = 10;
    p2.y = 20;

    if (p1 == p2) {
        println("p1 == p2: true");
    } else {
        println("p1 == p2: false");
    }

    return 0;
}
