// プリミティブ型へのimplテスト

interface Abs {
    int abs();
}

impl int for Abs {
    int abs() {
        if (self < 0) {
            return -self;
        }
        return self;
    }
}

int main() {
    int a = 5;
    int b = -3;
    int c = 0;
    
    int abs_a = a.abs();
    int abs_b = b.abs();
    int abs_c = c.abs();
    println("{abs_a}");
    println("{abs_b}");
    println("{abs_c}");
    
    return 0;
}
