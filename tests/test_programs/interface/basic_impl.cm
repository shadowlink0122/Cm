import std::io::println;
// 基本的なinterface/impl テスト

interface Printable {
    void print();
}

struct Point {
    int x;
    int y;
}

impl Point for Printable {
    void print() {
        println("Point({self.x}, {self.y})");
    }
}

int main() {
    Point p;
    p.x = 10;
    p.y = 20;
    p.print();
    return 0;
}
