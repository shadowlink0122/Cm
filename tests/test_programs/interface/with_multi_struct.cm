// 複数の構造体に対するEq自動実装テスト
struct Point with Eq {
    int x;
    int y;
}

struct Rect with Eq {
    int x;
    int y;
    int width;
    int height;
}

int main() {
    Point p1;
    p1.x = 10;
    p1.y = 20;
    
    Point p2;
    p2.x = 10;
    p2.y = 20;
    
    if (p1 == p2) {
        println("Point: equal");
    } else {
        println("Point: not equal");
    }
    
    Rect r1;
    r1.x = 0;
    r1.y = 0;
    r1.width = 100;
    r1.height = 50;
    
    Rect r2;
    r2.x = 0;
    r2.y = 0;
    r2.width = 100;
    r2.height = 50;
    
    Rect r3;
    r3.x = 0;
    r3.y = 0;
    r3.width = 200;
    r3.height = 50;
    
    if (r1 == r2) {
        println("Rect r1==r2: equal");
    } else {
        println("Rect r1==r2: not equal");
    }
    
    if (r1 == r3) {
        println("Rect r1==r3: equal");
    } else {
        println("Rect r1==r3: not equal");
    }
    
    return 0;
}
