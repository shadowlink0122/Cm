// 複数メソッドのimplテスト

interface Geometry {
    int area();
    int perimeter();
}

struct Rectangle {
    int width;
    int height;
}

impl Rectangle for Geometry {
    int area() {
        return self.width * self.height;
    }
    
    int perimeter() {
        return 2 * (self.width + self.height);
    }
}

int main() {
    Rectangle r;
    r.width = 10;
    r.height = 5;
    
    println("Area: {}", r.area());
    println("Perimeter: {}", r.perimeter());
    
    return 0;
}
