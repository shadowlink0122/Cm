// 複数メソッドのimplテスト

interface Geometry {
    int area();
    int perimeter();
}

struct Rectangle {
    int width;
    int height;
}

impl Rectangle for Geometry {
    int area() {
        return self.width * self.height;
    }
    
    int perimeter() {
        return 2 * (self.width + self.height);
    }
}

int main() {
    Rectangle r;
    r.width = 10;
    r.height = 5;
    
    int a = r.area();
    int p = r.perimeter();
    println("Area: {a}");
    println("Perimeter: {p}");
    
    return 0;
}
