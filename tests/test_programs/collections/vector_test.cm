// 最小限のVector<T>テスト（malloc/freeなし）
import std::io::println;

struct Vector<T> {
    T* data;
    int size;
    int cap;
}

impl Vector<T> {
    void init() {
        this.data = 0 as T*;
        this.size = 0;
        this.cap = 0;
    }

    int len() {
        return this.size;
    }
}

int main() {
    println("=== Minimal Vector Test ===");

    Vector<int> vi;
    vi.init();
    int len1 = vi.len();
    println("len: {len1}");

    println("=== PASS ===");
    return 0;
}
