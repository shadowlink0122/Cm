import std::io::println;
// アロー演算子テスト (ptr->member)

struct Point {
    int x;
    int y;
}

int main() {
    Point p = {x: 10, y: 20};
    Point* ptr = &p;
    
    // アロー演算子で読み取り
    println("ptr->x = {ptr->x}");
    println("ptr->y = {ptr->y}");
    
    // アロー演算子で書き込み
    ptr->x = 100;
    ptr->y = 200;
    println("after modify: p.x = {p.x}");
    println("after modify: p.y = {p.y}");
    
    return 0;
}
