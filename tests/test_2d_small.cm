import std::io;
int main() {
    int[100][100] a;
    int[100][100] b;
    for (int i = 0; i < 100; i++) {
        for (int j = 0; j < 100; j++) {
            a[i][j] = i + j;
            b[i][j] = i * j;
        }
    }
    int sum = 0;
    for (int i = 0; i < 100; i++) {
        for (int j = 0; j < 100; j++) {
            sum = sum + a[i][j] + b[i][j];
        }
    }
    println("{sum}");
    return 0;
}