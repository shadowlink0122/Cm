// Test: Swap elements through pointer
import std::io;

void swap_elements(int* arr) {
    int temp = arr[0];
    arr[0] = arr[1];
    arr[1] = temp;
}

int main() {
    int[2] data = [10, 20];
    println("Before: {data[0]}, {data[1]}");
    
    swap_elements(data);
    
    println("After: {data[0]}, {data[1]}");
    return 0;
}
