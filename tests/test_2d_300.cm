import std::io;
void matrix_multiply(int n) {
    int[300][300] a;
    int[300][300] b;
    int[300][300] c;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            a[i][j] = (i + j) % 100;
            b[i][j] = (i - j + 100) % 100;
            c[i][j] = 0;
        }
    }
    for (int i = 0; i < n; i++) {
        for (int k = 0; k < n; k++) {
            int a_ik = a[i][k];
            for (int j = 0; j < n; j++) {
                c[i][j] = c[i][j] + a_ik * b[k][j];
            }
        }
    }
    println("c[0][0] = {c[0][0]}");
}
int main() {
    matrix_multiply(300);
    return 0;
}