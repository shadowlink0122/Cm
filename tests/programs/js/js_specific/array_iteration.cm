// JS-specific: Array methods test
int main() {
    int[5] arr = [5, 3, 1, 4, 2];

    // Basic iteration
    println("Array:");
    for (int i = 0; i < 5; i = i + 1) {
        int v = arr[i];
        println("  [{i}] = {v}");
    }

    // Sum
    int sum = 0;
    for (int i = 0; i < 5; i = i + 1) {
        sum = sum + arr[i];
    }
    println("Sum: {sum}");

    return 0;
}
