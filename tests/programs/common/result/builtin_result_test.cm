// Result型のテスト（ユーザー定義）
import std::io::println;

// Result型を明示的に定義
enum Result<T, E> {
    Ok(T),
    Err(E)
}

Result<int, string> safe_divide(int a, int b) {
    if (b == 0) {
        return Result::Err("Division by zero");
    }
    return Result::Ok(a / b);
}

int main() {
    println("=== Result Type Test ===");

    // 成功ケース
    Result<int, string> r1 = safe_divide(10, 2);
    match (r1) {
        Result::Ok(v) => { println("10 / 2 = {v}"); }
        Result::Err(e) => { println("Err: {e}"); }
    }

    // 失敗ケース
    Result<int, string> r2 = safe_divide(10, 0);
    match (r2) {
        Result::Ok(v) => { println("10 / 0 = {v}"); }
        Result::Err(e) => { println("Err: {e}"); }
    }

    println("=== PASS ===");
    return 0;
}
