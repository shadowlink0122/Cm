import std::io::println;
// 文字列の.len()メソッドテスト

int main() {
    string s = "Hello";
    uint len = s.len();
    println(len);
    // 5

    string empty = "";
    println(empty.len());
    // 0

    string long_str = "Hello, World!";
    println(long_str.len());
    // 13

    // .size() と .length() エイリアス
    println(s.size());
    // 5
    println(s.length());
    // 5

    return 0;
}
