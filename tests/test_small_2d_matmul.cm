// Small 2D matrix test
import std::io::println;

int main() {
    int n = 10;
    int[10][10] a;
    int[10][10] b;
    int[10][10] c;
    
    // Initialize
    for (int i = 0; i < n; i = i + 1) {
        for (int j = 0; j < n; j = j + 1) {
            a[i][j] = i + j;
            b[i][j] = i * j;
            c[i][j] = 0;
        }
    }
    
    // Matrix multiply
    for (int i = 0; i < n; i = i + 1) {
        for (int j = 0; j < n; j = j + 1) {
            for (int k = 0; k < n; k = k + 1) {
                c[i][j] = c[i][j] + a[i][k] * b[k][j];
            }
        }
    }
    
    println("c[0][0] = {c[0][0]}");
    println("c[5][5] = {c[5][5]}");
    return 0;
}
