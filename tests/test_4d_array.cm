// 4D Array benchmark
import std::io;

int main() {
    // 4D配列: 5x5x5x5 = 625 要素
    int[5][5][5][5] arr4d;
    
    // 初期化
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) {
            for (int k = 0; k < 5; k++) {
                for (int l = 0; l < 5; l++) {
                    arr4d[i][j][k][l] = i * 125 + j * 25 + k * 5 + l;
                }
            }
        }
    }
    
    // 合計を計算
    int sum = 0;
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) {
            for (int k = 0; k < 5; k++) {
                for (int l = 0; l < 5; l++) {
                    sum = sum + arr4d[i][j][k][l];
                }
            }
        }
    }
    
    // 期待値: 0 + 1 + 2 + ... + 624 = 624*625/2 = 195000
    println("4D array sum: {sum}");
    println("Expected: 195000");
    
    // 特定の要素をテスト
    println("arr4d[0][0][0][0] = {arr4d[0][0][0][0]}");  // 0
    println("arr4d[1][2][3][4] = {arr4d[1][2][3][4]}");  // 1*125 + 2*25 + 3*5 + 4 = 194
    println("arr4d[4][4][4][4] = {arr4d[4][4][4][4]}");  // 624
    
    return 0;
}
