import std::io;
int main() {
    int[1000] a;
    int[1000] b;
    int[1000] c;
    
    // 初期化
    for (int i = 0; i < 1000; i++) {
        a[i] = i;
        b[i] = i * 2;
    }
    
    // 単純なベクトル加算（ベクトル化されるべき）
    for (int i = 0; i < 1000; i++) {
        c[i] = a[i] + b[i];
    }
    
    println("{c[500]}");
    return 0;
}
