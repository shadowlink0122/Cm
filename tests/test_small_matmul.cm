import std::io;
int main() {
    int[50][50] A;
    int[50][50] B;
    int[50][50] C;
    
    for (int i = 0; i < 50; i++) {
        for (int j = 0; j < 50; j++) {
            A[i][j] = i + j;
            B[i][j] = i * j;
            C[i][j] = 0;
        }
    }
    
    for (int i = 0; i < 50; i++) {
        for (int j = 0; j < 50; j++) {
            for (int k = 0; k < 50; k++) {
                C[i][j] = C[i][j] + A[i][k] * B[k][j];
            }
        }
    }
    
    println("{C[25][25]}");
    return 0;
}
