// 最適化レベルが実際に適用されるかのテスト
// シンプルなコードで、パターン検出器に引っかからないようにする

int add(int a, int b) {
    return a + b;
}

int multiply(int x, int y) {
    return x * y;
}

int factorial(int n) {
    if (n <= 1) {
        return 1;
    }
    return n * factorial(n - 1);
}

int main() {
    int a = 10;
    int b = 20;
    int sum = add(a, b);
    int prod = multiply(sum, 2);
    int fact = factorial(5);

    // 簡単な最適化可能なコード
    int x = 1 + 2;  // 定数畳み込み対象
    int y = x * 2;  // 定数伝播対象
    int z = y + 3;  // 定数畳み込み対象

    // デッドコード（最適化で削除されるはず）
    int unused1 = 100;
    int unused2 = 200;

    return z + fact + prod;
}