{
  "name": "cm-language",
  "displayName": "Cm Language Support",
  "description": "Syntax highlighting and language support for the Cm programming language",
  "version": "0.14.0",
  "publisher": "cm-lang",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "cm",
    "programming language",
    "syntax highlighting",
    "systems programming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shadowlink0122/Cm"
  },
  "license": "MIT",
  "icon": "images/icon.svg",
  "scripts": {
    "update-version": "node scripts/update-version.js",
    "verify-version": "node scripts/verify-version.js",
    "prepackage": "npm run verify-version",
    "package": "vsce package"
  },
  "contributes": {
    "languages": [
      {
        "id": "cm",
        "aliases": [
          "Cm",
          "cm"
        ],
        "extensions": [
          ".cm"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/icon.svg",
          "dark": "./images/icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "cm",
        "scopeName": "source.cm",
        "path": "./syntaxes/cm.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "cm-icons",
        "label": "Cm Icons",
        "path": "./iconTheme.json"
      }
    ]
  }
}